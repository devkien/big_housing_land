<IfModule mod_rewrite.c>
    RewriteEngine On
    RewriteBase /

    # Serve existing files and directories directly
    RewriteCond %{REQUEST_FILENAME} -f [OR]
    RewriteCond %{REQUEST_FILENAME} -d
    RewriteRule ^ - [L]

    # Route other requests to front controller
    RewriteRule ^ index.php [L,QSA]
</IfModule>